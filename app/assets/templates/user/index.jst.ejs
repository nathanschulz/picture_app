<div class="row">
  <% posts.forEach(function(post){ %>
    <div class="col-xs-3">
      <a 
        href="#cool-modal" 
        class="thumbnail" 
        data-id="<%= post.attributes.id %>"
        data-toggle="modal"
        class="btn btn-primary">
        <img src="<%= post.attributes.filepicker_url %>" alt="200x200">
      </a>
    </div>
  <% }) %>
</div>


 <div id="show-view"></div>