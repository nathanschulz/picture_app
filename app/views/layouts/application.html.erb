<!DOCTYPE html>
<html>
<head>
  <title>PictureApp</title>
  
  <%= stylesheet_link_tag    'application'%>
  <%= javascript_include_tag 'application'%>  
  <%= csrf_meta_tags %>

  
</head>
  
 <script 
   type="text/javascript"
   src="//api.filepicker.io/v1/filepicker.js">
 </script>
 
 

<body>

   <nav class="navbar navbar-default" role="navigation">
    <div class="container-fluid">
      <div class="navbar-header">
        <a class="navbar-brand" href="#">Instagrad</a>
      </div>

      <!-- Collect the nav links, forms, and other content for toggling -->
      <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
        <ul class="nav navbar-nav navbar-right">
          <% unless logged_in? %>
            <li><%= link_to "Login", new_session_url %></li>
            <li><%= link_to "Sign Up", new_user_url %></li>
          <% else %>
            <li><%= link_to "Log Out", session_url, method: :delete %></li>
          <% end %>
          
          <% unless "i love you" %>
          
          <li><%= link_to "Log Out", session_url, method: :delete %></li>
          
          
          
          <li class="dropdown">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown">Dropdown <span class="caret"></span></a>
            <ul class="dropdown-menu" role="menu">
              <li><a href="#">Action</a></li>
              <li><a href="#">Another action</a></li>
              <li><a href="#">Something else here</a></li>
              <li class="divider"></li>
              <li><a href="#">Separated link</a></li>
            </ul>
          </li>
          <% end %>
        </ul>
      </div><!-- /.navbar-collapse -->
    </div><!-- /.container-fluid -->
<% if flash[:errors] %>
<div class="alert alert-warning alert-dismissible" role="alert">
   <button type="button" class="close" data-dismiss="alert"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
<%= flash[:errors].join("<br>").html_safe %>
</div>
<% end %>

  </nav>
  
  <%= yield %>
  
  <div class="container">
    
    
    
  </div>





</body>
</html>
